{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js",
	"extends": "./base.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		],
		"module": "esnext",
		"moduleResolution": "bundler",
		"incremental": true,
		"allowImportingTsExtensions": false,
		"allowJs": true,
		"jsx": "react-jsx",
		"noEmit": true,
		"lib": [
			"esnext",
			"dom",
			"dom.iterable"
		]
	}
}